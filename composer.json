{
    "name": "ubitransport/vcr-bundle",
    "type": "symfony-bundle",
    "description": "Integrates php-vcr into Symfony and its web profiler.",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2|^8.0",
        "php-vcr/php-vcr": "^1.5",
        "php-vcr/phpunit-testlistener-vcr": "^3.3",
        "symfony/config": "^6|^7",
        "symfony/browser-kit": "^6|^7",
        "symfony/dependency-injection": "^6|^7",
        "symfony/filesystem": "^6|^7",
        "symfony/event-dispatcher": "^6|^7",
        "symfony/framework-bundle": "^6|^7",
        "symfony/http-foundation": "^6|^7",
        "symfony/http-kernel": "^6|^7",
        "symfony/polyfill-php80": "^1.16",
        "symfony/yaml": "^6|^7"
    },
    "require-dev": {
        "dms/phpunit-arraysubset-asserts": "^0.4",
        "neutron/temporary-filesystem": "^3",
        "phpunit/phpunit": "^8.5|^9.5",
        "symfony/phpunit-bridge": "^4.4|^5.4"
    },
    "autoload": {
        "psr-4": {
            "VCR\\VCRBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VCR\\VCRBundle\\Tests\\Functional\\App\\": "tests/Functional/app",
            "VCR\\VCRBundle\\Tests\\": "tests/"
        }
    }
}
